<%- model_class = Knowandcul -%>
<div class="page-header">
<!--   <h1><%=t '.title', :default => model_class.model_name.human.pluralize %></h1> -->
<h1 align="center"><%= 'Knowledge and Culture' %></h1></br >

<!-- <h4><span style="padding-left:80%"><%= 'Knowledge and Culture' %></span></h4> -->
<div class="container">
  <div class="row">
    <div class="col-md-10">
      <h4><b><%= 'Know your clients:' %></b><i><%= ' Pratik, Javed'%></i></h4>
    </div>
    <div class="col-md-2">
      <h4><%= link_to "Create an Idea",
            new_knowandcul_path %></span></h4>
    </div>
  </div>
</div>

</div>
<style>
   table {border-collapse:collapse; table-layout:fixed; width:800px;}
   table td { word-wrap:break-word;}
   table th { text-align: center;}
</style>
<table class="table table-striped">
  <colgroup>
    <col class="col-md-1">
    <col class="col-md-2">
    <col class="col-md-2">
    <col class="col-md-1">
    <col class="col-md-1">
    <col class="col-md-1">
    <col class="col-md-2">
    <col class="col-md-1">
    <col class="col-md-1">    
  </colgroup>
  <thead>
    <tr>
      <!-- <th><%= model_class.human_attribute_name(:id) %></th> -->
      <th><%= model_class.human_attribute_name(:Name) %></th>
      <th><%= model_class.human_attribute_name(:Summary) %></th>
      <th><%= model_class.human_attribute_name(:Description) %></th>
      <th><%= model_class.human_attribute_name(:Owner) %></th>
      <th><%= model_class.human_attribute_name("Reviewed by") %></th>
      <th><%= model_class.human_attribute_name(:Status_id) %></th>
      <th><%= model_class.human_attribute_name(:Comments) %></th>
      <th><%= model_class.human_attribute_name("Action Item") %></th>
      <!-- <th><%= model_class.human_attribute_name("Imperative Owner") %></th> -->
      <!-- <th><%= model_class.human_attribute_name(:created_at) %></th> -->
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @knowandculs.each do |knowandcul| %>
      <tr>
        <% if knowandcul.ImpOwner_id == 1 %>
        <!-- <td><%= knowandcul.id %></td> -->
        <!-- <td><%= knowandcul.Name %></td> -->
        <td><%= link_to knowandcul.Name, knowandcul_path(knowandcul) %></td>
        <td><%= auto_link(knowandcul.Summary) %></td>
        <td><%= auto_link(knowandcul.Description) %></td>
        <td><%= knowandcul.Owner %></td>
        <td><%= knowandcul.Reviewed %></td>
        <td><%= knowandcul.GetStatus %></td>
        <td><%= auto_link(knowandcul.Comments) %></td>
        <td><%= knowandcul.Action %></td>
        <!-- <td><%= knowandcul.GetOwner %></td> -->
        <!-- <td><%=l knowandcul.created_at %></td> -->
        <td align="center">
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_knowandcul_path(knowandcul) %><br>
          <%= link_to t('.destroy', :default => t("Delete")),
                      knowandcul_path(knowandcul),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) } %>
        </td>        
      </tr>
      <% end %>
    <% end %>
  </tbody>
</table>

